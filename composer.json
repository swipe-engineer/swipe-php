{
    "name": "swipe/swipe-php",
    "description": "Swipe PHP Library",
    "homepage": "https://swipego.io/",
    "license": "MIT",
    "authors": [{
        "name": "team swipe",
        "email": "team@swipego.io"
    }],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Swipego\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^9.0",
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "3.5.0",
        "phpstan/phpstan": "^1.2"
    }
}
